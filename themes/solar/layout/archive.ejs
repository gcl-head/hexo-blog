<ul class="post-list">
    <% page.posts.sort('updated', 'desc').each(function(post) { %>
    <li class="post-item">
      <%- partial('_partial/post/updated', { post: post, class_name: 'meta' }) %>
      <span><%- partial('_partial/post/title', { post: post, index: true, class_name: '' }) %></span>
    </li>
    <% }); %>

    <%- partial('_partial/pagination') %>
</ul>

